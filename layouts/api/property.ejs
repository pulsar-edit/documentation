<% for (const item of content) { %>
  <div class="api__entry" id="<%= anchorize(item.name) %>">
    <h4 class="api__entry__name">
      <a href="#<%= anchorize(item.name) %>">
        <span><%= locals.separator ?? '::' -%><%= item.name %></span>
      </a>
    </h4>
    <div class="api-entry-summary">
      <%- mdRender(item.summary) %>
    </div>
  </div>
<% } %>
